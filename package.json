{
  "name": "my_project",
  "type": "module",
  "version": "0.0.1",
  "packageManager": "pnpm@9.12.0",
  "description": "TypeScript library template",
  "author": "zAlweNy26",
  "license": "MIT",
  "homepage": "https://github.com/me/my_project",
  "repository": {
    "type": "git",
    "url": "https://github.com/me/my_project.git"
  },
  "bugs": {
    "url": "https://github.com/me/my_project/issues"
  },
  "keywords": [
    "library",
    "typescript",
    "node"
  ],
  "exports": {
    "import": {
      "types": "./dist/index.d.mts",
      "default": "./dist/index.mjs"
    },
    "require": {
      "types": "./dist/index.d.cts",
      "default": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.cts",
  "files": [
    "/dist"
  ],
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "typecheck": "tsc --noEmit",
    "build": "pkgroll --minify --clean-dist",
    "prepare": "simple-git-hooks",
    "start": "tsx ./src/index.ts",
    "watch": "tsx watch ./src/index.ts",
    "test": "vitest dev --run",
    "test:ui": "vitest dev --ui",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "docs:dev": "typedoc & vitepress dev docs",
    "docs:build": "typedoc & vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.8.0",
    "@types/node": "^22.8.0",
    "@vitest/ui": "^2.1.3",
    "changelogen": "^0.5.7",
    "eslint": "^9.13.0",
    "eslint-plugin-oxlint": "^0.10.1",
    "lint-staged": "^15.2.10",
    "oxlint": "^0.10.2",
    "pkgroll": "^2.5.0",
    "simple-git-hooks": "^2.11.1",
    "tsx": "^4.19.1",
    "typedoc": "^0.26.10",
    "typedoc-plugin-coverage": "^3.3.0",
    "typedoc-plugin-markdown": "^4.2.9",
    "typedoc-plugin-merge-modules": "^6.0.3",
    "typedoc-vitepress-theme": "^1.0.2",
    "typescript": "^5.6.3",
    "vitepress": "^1.4.1",
    "vitest": "^2.1.3",
    "vue": "^3.5.12"
  },
  "simple-git-hooks": {
    "pre-commit": "npm run typecheck & npx lint-staged"
  },
  "lint-staged": {
    "**/*.{js,mjs,cjs,jsx,ts,mts,cts}": "oxlint"
  },
  "changelog": {
    "types": {
      "feat": {
        "title": "ðŸš€ Enhancements",
        "semver": "patch"
      },
      "refactor": {
        "title": "ðŸ’… Refactors",
        "semver": "minor"
      }
    }
  }
}
